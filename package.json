{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "prisma generate --config ./prisma.config.ts && nest build",
    "start": "prisma migrate deploy --config ./prisma.config.ts && node dist/src/main",
    "start:prod": "node dist/src/main",
    "dev": "nest start --watch",
    "prisma:generate": "prisma generate --config ./prisma.config.ts",
    "prisma:migrate": "prisma migrate dev --config ./prisma.config.ts",
    "prisma:studio": "prisma studio --config ./prisma.config.ts"
  },
  "dependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@upstash/redis": "^1.34.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
